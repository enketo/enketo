{
    "name": "enketo",
    "description": "Enketo Web Forms",
    "private": true,
    "workspaces": {
        "packages": [
            "packages/openrosa-xpath-evaluator",
            "packages/enketo-transformer",
            "packages/enketo-core",
            "packages/enketo-express",
            "examples/*"
        ],
        "nohoist": [
            "**/bootstrap-datepicker",
            "**/vex-js"
        ]
    },
    "scripts": {
        "build": "yarn workspaces run build",
        "watch:express": "yarn workspace enketo-express develop",
        "watch:transformer:web": "yarn workspace enketo-transformer watch:web",
        "watch:transformer:node": "yarn workspace enketo-transformer watch:node",
        "watch:forms": "serve --cors packages/enketo-core/test/forms/",
        "watch": "concurrently -k 'npm:watch:*'",
        "lint": "eslint --cache --quiet .",
        "test": "yarn workspaces run test",
        "postinstall": "yarn build"
    },
